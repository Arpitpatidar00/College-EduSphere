{
  "name": "college-project",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "pnpm --filter client dev",
    "dev:server": "pnpm --filter server start",
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:server\"",
    "build:client": "pnpm --filter client build",
    "build:server": "pnpm --filter server build",
    "build": "concurrently \"pnpm build:client\" \"pnpm build:server\"",
    "clean:client": "rm -rf client/dist",
    "clean:server": "rm -rf server/dist",
    "clean": "rm -rf node_modules client/dist server/dist && pnpm install",
    "start": "pnpm --filter server start",
    "restart": "pnpm clean && pnpm build && pnpm start"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "module-alias": "^2.2.3",
    "react-router-dom": "^7.0.0"
  },
  "_moduleAliases": {
    "@config": "src/config",
    "@database": "src/database",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@routes": "src/routes"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
